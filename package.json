{
  "name": "samui",
  "version": "1.0.0",
  "description": "SAM UI framework using Angular 2 ",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal-starter.git"
  },
  "scripts": {
    "watch": "webpack --watch",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build:prod": "webpack --progress -p",
    "prestart": "npm run build",
    "server": "nodemon dist/server/index.js",
    "start": "npm run server",
    "predebug": "npm run build",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug": "node --debug-brk dist/server/index.js",
    "test": "karma start > tests.log"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "angular2-universal": "~0.104.5",
    "body-parser": "^1.15.2",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.0",
    "express": "^4.14.0",
    "methods": "~1.1.2",
    "parse5": "^1.5.0",
    "preboot": "^2.1.2",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "samwds": "^0.2.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.29",
    "@types/compression": "0.0.29",
    "@types/cookie-parser": "^1.3.29",
    "@types/es6-shim": "0.0.28",
    "@types/express": "^4.0.29",
    "@types/express-serve-static-core": "^4.0.29",
    "@types/mime": "0.0.28",
    "@types/node": "^4.0.30",
    "@types/serve-static": "^1.7.27",
    "angular2-template-loader": "^0.4.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^3.0.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "nodemon": "^1.10.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.4",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.0-beta",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.13.0"
  }
